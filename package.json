{
  "name": "vibe-debug",
  "displayName": "vibe-debug",
  "description": "An Extension that lets you generate complete contexts for your codebase.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vibe-debug.generate",
        "title": "Generate Debug Context"
      },
      {
        "command": "extension.pasteTerminalLogs",
        "title": "Paste Terminal Logs into Context"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "vibe-debug.generate",
          "group": "navigation"
        }
      ]
    },
    "keybindings": {
      "command": "extension.pasteTerminalLogs",
      "key": "ctrl+shift+l",
      "mac": "cmd+shift+l"
    }
  },
  "scripts": {
    "build": "node esbuild.js",
    "watch": "node esbuild.js --watch",
    "lint": "eslint src --ext ts",
    "pretest": "npm run lint",
    "test": "tsc && node ./out/test/runTest.js"
  },
  "dependencies": {
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "esprima": "^4.0.1",
    "fs-extra": "^11.1.0",
    "ignore": "^7.0.3",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.7",
    "@types/esprima": "^4.0.6",
    "@types/estree": "^1.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^16.18.68",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.25.2",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4"
  }
}
